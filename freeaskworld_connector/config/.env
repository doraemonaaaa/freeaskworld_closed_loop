# FreeAskAgent Closed-Loop Default Configuration
# Recommended settings for local development

# Inference Control (2s interval = ~0.5 calls/sec)
MIN_RESPONSE_INTERVAL=2.0

# WebRTC Video (30fps, 2Mbps for local network)
WEBRTC_FRAMERATE=30
WEBRTC_BITRATE=2000000

# Preview (10fps update for smooth preview)
JPEG_UPDATE_INTERVAL=0.1
PREVIEW_PORT=8080
PREVIEW_AUTO_OPEN=0

# WebSocket Server
WS_HOST=0.0.0.0
WS_PORT=8766
WS_PING_INTERVAL=20
WS_PING_TIMEOUT=30
WS_MAX_SIZE=30485760

# WebRTC ICE (localhost config)
ICE_TRANSPORT_POLICY=all
ICE_SERVERS=[{"urls":"stun:stun.l.google.com:19302"}]

# AgentFlow
LLM_ENGINE_NAME=gpt-4o
FAST_MODE=true

# Debugging
LOG_LEVEL=INFO
